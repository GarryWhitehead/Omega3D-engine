{
	"DepthStencilState": {
		"DepthTestEnable": true,
		"DepthWriteEnable": true,
		"CompareOp": "LessOrEqual"
	},

	"RasterState": {
		"PolygonMode": "Fill",
		"CullMode": "Back",
		"FrontFace": "CounterClockwise"
	},
	
	"Sampler": {
		"MagFilter": "Nearest",
		"MinFilter": "Nearest",
		"AddressModeU": "ClampToEdge",
		"AddressModeV": "ClampToEdge",
		"AddressModeW": "ClampToEdge"
	},
	
	"CodePath": "irradianceMap.glsl",
	
	"VertexShader": {
		"PushConstants": [
			{ "name": "PushConstant", "id": "push", "items": [ { "name": "mvp", "type": "mat4" } ] }
		],
		"Inputs": [
			{ "name": "Pos", "type": "vec3" }
		],
		"Outputs": [
			{ "name": "Uv", "type": "vec3" } 
		]
	},
	
	"FragmentShader": {
		"Imports": [
			{ "name": "envSampler", "type": "Cube_Sampler" }
		],
		"Outputs": [
			{ "name": "Col", "type": "vec4" }
		]
	}
}
